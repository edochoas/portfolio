{
  "name": "currency-exchange",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/currency-exchange/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/currency-exchange",
        "main": "apps/currency-exchange/src/main.ts",
        "tsConfig": "apps/currency-exchange/tsconfig.app.json",
        "assets": ["apps/currency-exchange/src/assets"],
        "webpackConfig": "apps/currency-exchange/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/currency-exchange/src/environments/environment.ts",
              "with": "apps/currency-exchange/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "currency-exchange:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "currency-exchange:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/currency-exchange/jest.config.ts"
      }
    }
  }
}
